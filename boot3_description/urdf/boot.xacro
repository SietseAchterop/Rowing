<?xml version="1.0" ?>

<robot name="myboat"  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Constants for robot dimensions -->


  <!-- Import all Gazebo-customization elements, including Gazebo colors -->
  <xacro:include filename="$(find boot3_description)/urdf/boot.gazebo" />
  <!-- Import Rviz colors -->
  <xacro:include filename="$(find boot3_description)/urdf/materials.xacro" />

  <!-- parameters that can be configured -->
  <xacro:include filename="$(find boot3_description)/config/param.xacro" />

  <joint name="prism" type="fixed">  <!-- prismatic -->
    <parent link="world"/>
    <child link="l1"/>
    <origin xyz="0 0 0.25" rpy="0 0 0"/>
    <dynamics damping="0.005" />
    <axis xyz="1 0 0"/>
    <limit effort="101.0" lower="0" upper="1000" velocity="0.5"/>
  </joint>

  <joint name="rev1" type="revolute">
    <parent link="l1"/>
    <child link="l2"/>
    <origin xyz="0.0 0 -0.2" rpy="0 ${pi/2} 0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>

  <joint name="rev2" type="revolute">
    <parent link="l2"/>
    <child link="skiff"/>
    <origin xyz="0.0 0.0 0.4" rpy="${pi} ${pi/2} 0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1" upper="1" velocity="1.0"/>
  </joint>

  <joint name="starboard_rowlock" type="revolute">
    <parent link="skiff"/>
    <child link="starboard_rowlock_l"/>
    <origin rpy="0.0  0.0 -0.61" xyz="-3 ${span/2} 0.42"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-0.4" upper="1.4" velocity="1.0"/>
  </joint>

  <joint name="port_rowlock" type="revolute">
    <parent link="skiff"/>
    <child link="port_rowlock_l"/>
    <origin rpy="0.0  0.0 0.0" xyz="-3 -${span/2} 0.4"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1.4" upper="1.4" velocity="1.0"/>
  </joint>

  <joint name="starboard_oar" type="revolute">
    <parent link="starboard_rowlock_l"/>
    <child link="starboard_oar_l"/>
    <origin rpy="0.0  ${pi/2-0.13} -${pi/2}" xyz="0.0 0.0   0.02"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.4" upper="0.1" velocity="1.0"/>
  </joint>

  <joint name="port_oar" type="revolute">
    <parent link="port_rowlock_l"/>
    <child link="port_oar_l"/>
    <origin rpy="0.0  ${pi/2} -${pi/2}" xyz="0.0 0.0   0.02"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="0.0" upper="0.4" velocity="1.0"/>
  </joint>

  <joint name="starboard_blade" type="revolute">
    <parent link="starboard_oar_l"/>
    <child link="starboard_blade_l"/>
    <origin rpy="0 0  0.0" xyz="0.0 0.0   -${outer}"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <joint name="port_blade" type="revolute">
    <parent link="port_oar_l"/>
    <child link="port_blade_l"/>
    <origin rpy="0.0  0 0.0" xyz="0.0 0.0   ${outer}"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <!-- Rower joints -->

  <joint name="sk_fp" type="fixed">
    <parent link="skiff"/>
    <child link="footplate"/>
    <origin rpy="0.0  0 0.0" xyz="-2.5 0.0  0.2"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-0.4" upper="0.4" velocity="1.0"/>
  </joint>

  <joint name="sliding" type="prismatic">
    <parent link="skiff"/>
    <child link="seat"/>
    <origin rpy="0 0 0" xyz="-3.495 0.0 0.2"/>
    <dynamics damping="0.005" />
    <axis xyz="1 0 0"/>
    <limit effort="101.0" lower="-0.1" upper="0.8" velocity="0.5"/>
  </joint>

  <joint name="fp_ll" type="revolute">
    <parent link="footplate"/>
    <child link="lowerleg"/>
    <origin rpy="0.0  0.0998 0.0" xyz="0.0 0.0   0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="0.0" upper="${pi/2}" velocity="1.0"/>
  </joint>

  <joint name="knee" type="revolute">
    <parent link="lowerleg"/>
    <child link="upperleg_1"/>
    <origin rpy="0.0  0 0.0" xyz="-0.5 0.0   0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-3.1" upper="0" velocity="1.0"/>
  </joint>

  <joint name="hip1" type="revolute">
    <parent link="seat"/>
    <child link="upperleg_2"/>
    <origin rpy="0  0.0998 0.0" xyz="0.0 0.0   0.1"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-1.6" upper="0" velocity="1.0"/>
  </joint>

  <joint name="hip2" type="revolute">
    <parent link="seat"/>
    <child link="back_low"/>
    <origin rpy="0  -${pi/2+0.34} 0.0" xyz="0.0 0.0   0.1"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>

  <joint name="back_lh" type="revolute">
    <parent link="back_low"/>
    <child link="back_high"/>
    <origin rpy="0  0.0 0.0" xyz="0.3 0.0   0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="0.0" upper="0.5" velocity="1.0"/>
  </joint>

  <joint name="shoulder" type="fixed">
    <parent link="back_high"/>
    <child link="shoulder_link"/>
    <origin rpy="0  0.0 0.0" xyz="0.3 0.0   0.0"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="0.0" upper="0.5" velocity="1.0"/>
  </joint>

  <!-- 3D joint left shoulder -->
  <joint name="shoulder_l" type="revolute" >
    <parent link="shoulder_link" />
    <child link="left_shoulder_xx_link" />
    <origin rpy="0.0 0.0 0.13" xyz="0.0 0.2   0.0"/>
    <axis xyz="0 0 1" />
    <dynamics damping="0.005" />
    <!--    <limit effort="101.0" lower="-1.5" upper="0.5" velocity="1.0"/> -->
    <limit effort="101.0" lower="-0.1" upper="0.1" velocity="1.0"/>    
  </joint>
  <link name="left_shoulder_xx_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="shoulder_iets" type="revolute" >
    <parent link="left_shoulder_xx_link" />
    <child link="left_shoulder_psi_link" />
    <origin rpy="-0.803 0.0 0.0" xyz="0.0 0.0   0.0"/>
    <axis xyz="1 0 0" />
    <dynamics damping="0.005" />
<!--    <limit effort="101.0" lower="-1.5" upper="1.5" velocity="1.0"/> -->
    <limit effort="101.0" lower="-0.2" upper="0.2" velocity="1.0"/>
  </joint>
  <link name="left_shoulder_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="LShoulderTheta" type="revolute" >
    <parent link="left_shoulder_psi_link"/>
    <child link="arm_l_upper"/>
      <origin rpy="0 0.68 0" xyz="0 0.0 0.0"/>
    <axis xyz="0 1 0" />
    <dynamics damping="0.005" />
<!--    <limit effort="101.0" lower="-2.0" upper="1.0" velocity="1.0"/> -->
    <limit effort="101.0" lower="-0.2" upper="0.2" velocity="1.0"/>
  </joint>

  <!-- 2D joint right shoulder -->
  <joint name="shoulder_r" type="revolute" >
    <parent link="shoulder_link" />
    <child link="right_shoulder_psi_link" />
    <origin rpy="0.0 ${pi}  0.0" xyz="0.0 -0.2   0.0"/>
    <axis xyz="0 0 1" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="0.5" velocity="1.0"/>
  </joint>
  <link name="right_shoulder_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="RShoulderTheta" type="revolute" >
    <parent link="right_shoulder_psi_link"/>
    <child link="arm_r_upper"/>
    <axis xyz="0 1 0" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.5" upper="1.0" velocity="1.0"/>
  </joint>

  <!-- 2D joint left elbow -->
  <joint name="LElbowTheta" type="revolute" >
    <parent link="arm_l_upper"/>
    <child link="left_elbow_psi_link"/>
    <origin rpy="0.0 -1.94  0.0" xyz="-0.35 0.0  0.0"/>
    <axis xyz="0 1 0" />
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-0.7" upper="2.0" velocity="1.0"/>
  </joint>
  <link name="left_elbow_psi_link">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="elbow_left" type="revolute">
    <parent link="left_elbow_psi_link"/>
    <child link="arm_l_lower_1"/>
    <origin rpy="0 0 -0.09" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 0 1"/>
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>

  <!-- hoe kom ik aan de andere kant van arm_lower_1 en 2 ?  -->
  <joint name="dummy_1" type="fixed">
    <parent link="arm_l_lower_1"/>
    <child link="dummy_link_1"/>
    <origin rpy="0 0 0.0" xyz="-0.2 0.0 0.0"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="dummy_link_1">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="dummy_2" type="fixed">
    <parent link="arm_l_lower_2"/>
    <child link="dummy_link_2"/>
    <origin rpy="${pi/2} ${pi}  0.0" xyz="-0.15 0.0 0.0"/>
    <dynamics damping="0.005" />
    <limit effort="101.0" lower="-1.0" upper="1.0" velocity="1.0"/>
  </joint>
  <link name="dummy_link_2">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>

  <!-- 3D handle joint -->
  <joint name="handle_left_j1" type="revolute">
    <parent link="starboard_oar_l"/>
    <child link="handle_left_l1"/>
    <origin rpy="0.209 0 ${pi/2}" xyz="0 0.0 ${inner}"/>
    <dynamics damping="0.005" />
    <axis xyz="1 0 0"/>
    <limit effort="101.0" lower="-0.2" upper="0.2" velocity="1.0"/>
  </joint>
  <link name="handle_left_l1">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="handle_left_j2" type="revolute">
    <parent link="handle_left_l1"/>
    <child link="handle_left_l2"/>
    <origin rpy="0 0 0.06" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0  0 1"/>
    <limit effort="101.0" lower="-0.2" upper="0.2" velocity="1.0"/>
  </joint>
  <link name="handle_left_l2">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.000000167" ixy="0" ixz="0" iyy="0.000000167" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="handle_left_j3" type="revolute">
    <parent link="handle_left_l2"/>
    <child link="arm_l_lower_2"/>
    <origin rpy="0 -0.16 0" xyz="0 0.0 0.0"/>
    <dynamics damping="0.005" />
    <axis xyz="0 1 0"/>
    <limit effort="101.0" lower="-0.2" upper="0.2" velocity="1.0"/>
  </joint>



  <joint name="head_joint" type="fixed">
    <parent link="shoulder_link"/>
    <child link="head"/>
    <origin rpy="0  0.0 0.0" xyz="0.2 0.0   0.0"/>
  </joint>

  
  <!-- Boat links -->

  <link name="world"/>
  
  <link name="l1">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 -0.1"/>
      <inertia ixx="0.000033" ixy="0" ixz="0" iyy="0.000033" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 -0.1"/>
      <geometry>
        <box size="0.01 0.01 0.20"/>
      </geometry>
    </visual>
  </link>

  <link name="l2">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0.0 0.2"/>
      <inertia ixx="0.000133" ixy="0" ixz="0" iyy="0.1=000133" iyz="0" izz="0.000000167"/> 
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.2"/>
      <geometry>
        <box size="0.01 0.01 0.40"/>
      </geometry>
    </visual>
  </link>

  <link name="skiff">
    <inertial>
      <mass value="${15 + weight}"/>   <!-- boat plus roeier -->
      <origin rpy="0 0 0" xyz="-3 0.0 0.05"/>
      <inertia ixx="3.2" ixy="0" ixz="0" iyy="225" iyz="0" izz="228"/>   <!-- weight= 60 -->
<!--      <inertia ixx="0.6" ixy="0" ixz="0" iyy="11" iyz="0" izz="11.8"/>  -->
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="-3 0.0   0.05"/>
      <geometry>
        <box size="6.0 0.7 0.2"/>
      </geometry>
    </collision>
    <visual>
      <!-- should be a nice skull shape including riggers-->
      <origin rpy="0 0 0" xyz="-3 0.0   0.05"/>
      <geometry>
        <box size="6.0 0.7 0.2"/>
      </geometry>
    </visual>
  </link>

  <link name="starboard_rowlock_l">    
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0" iyy="0.000017" iyz="0" izz="0.000007"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0   0.0"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0   0.0"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
    </visual>
  </link>

  <link name="port_rowlock_l">    
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.000017" ixy="0" ixz="0" iyy="0.000017" iyz="0" izz="0.000007"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0   0.0"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0   0.0"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
    </visual>
  </link>

  <link name="starboard_oar_l">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="0 0 -${(outer-inner)/2}"/>
      <inertia ixx="0.6" ixy="0" ixz="0" iyy="0.6" iyz="0" izz="0.001"/>	   
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0   0   -${(outer-inner)/2}"/>
      <geometry>
        <cylinder length="${inner+outer -0.6}" radius="0.02"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0   0.0   -${(outer-inner)/2}"/>
      <geometry>
        <cylinder length="${inner+outer}" radius="0.02"/>
      </geometry>
    </visual>
  </link>

  <link name="port_oar_l">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="0 0 ${(outer-inner)/2}"/>
      <inertia ixx="0.6" ixy="0" ixz="0" iyy="0.6" iyz="0" izz="0.001"/>	   
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0   0   ${(outer-inner)/2}"/>
      <geometry>
        <cylinder length="${inner+outer-0.6}" radius="0.02"/>  <!-- -0.6 to prevent collissions  -->
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0   0   ${(outer-inner)/2}"/>
      <geometry>
        <cylinder length="${inner+outer}" radius="0.02"/>
      </geometry>
    </visual>
  </link>

  <link name="starboard_blade_l">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 -1.4 0" xyz="0.02 0 -0.02"/>
      <inertia ixx="0.0009" ixy="0" ixz="0" iyy="0.0034" iyz="0" izz="0.004"/>
    </inertial>
    <collision>
      <origin rpy="0 -1.4 0" xyz="0.02    0   -0.02"/>
      <geometry>
        <box size="0.2 0.1 0.03"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 -1.4 0" xyz="0.02    0   -0.02"/>
      <geometry>
        <box size="0.2 0.1 0.03"/>
      </geometry>
    </visual>
  </link>

  <link name="port_blade_l">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 1.4 0" xyz="0.02 0 -0.02"/>
      <inertia ixx="0.0009" ixy="0" ixz="0" iyy="0.0034" iyz="0" izz="0.004"/>
    </inertial>
    <collision>
      <origin rpy="0 1.4 0" xyz="0.02    0   -0.02"/>
      <geometry>
        <box size="0.2 0.1 0.03"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 1.4 0" xyz="0.02    0.0  -0.02"/>
      <geometry>
        <box size="0.2 0.1 0.03"/>
      </geometry>
    </visual>
  </link>

  <!-- Rower links -->

  <link name="footplate">
    <inertial>
      <mass value="0.1"/> 
      <origin rpy="0 0 0" xyz="0 0.0 0.0"/>
      <inertia ixx="0.0008" ixy="0" ixz="0" iyy="0.000166" iyz="0" izz="0.0008"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0.0   0.0"/>
      <geometry>
        <box size="0.1 0.3 0.1"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0  0.0   0.0"/>
      <geometry>
        <box size="0.1 0.3 0.1"/>
      </geometry>
    </visual>
  </link>

  <link name="lowerleg">
