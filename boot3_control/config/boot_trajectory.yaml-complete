boot:
  # Publish all joint states -----------------------------------
  joint_state_controller:
    type: joint_state_controller/JointStateController
    publish_rate: 50  
  
  # Trajectory Controller  (not together with the position controllers)
  joint_trajectory_controller:
    type: effort_controllers/JointTrajectoryController
    # which joints to control:
    joints:
      - starboard_blade
      - port_blade
      - knee
      - hip2
      - back_lh
      - shoulder_l
      - shoulder_iets
      - LShoulderTheta

    constraints:
      goal_time: 0.5
      stopped_velocity_tolerance: 0.01
      starboard_blade        : {trajectory: 0.01, goal: 0.01}
      port_blade             : {trajectory: 0.01, goal: 0.01}
      knee                   : {trajectory: 0.01, goal: 0.01}
      hip2                   : {trajectory: 0.01, goal: 0.01}
      back_lh                : {trajectory: 0.01, goal: 0.01}
      shoulder_l             : {trajectory: 0.01, goal: 0.01}
      shoulder_iets          : {trajectory: 0.01, goal: 0.01}
      LShoulderTheta         : {trajectory: 0.01, goal: 0.01}
    gains:
      starboard_blade        :  {p: 100.0, d: 5.0, i: 0.0, i_clamp: 0.0}
      port_blade             :  {p: 100.0, d: 5.0, i: 0.0, i_clamp: 0.0}
      knee                   :  {p: 10000.0, d: 5.0, i: 0.0, i_clamp: 0.0}
      hip2                   :  {p: 1000.0, d: 100.0, i: 0.0, i_clamp: 0.0}
      back_lh                :  {p: 10000.0, d: 5.0, i: 0.0, i_clamp: 0.0}
      shoulder_l             :  {p: 1000.0, d: 100.0, i: 0.0, i_clamp: 0.0}
      shoulder_iets          :  {p: 1000.0, d: 100.0, i: 0.0, i_clamp: 0.0}
      LShoulderTheta         :  {p: 1000.0, d: 100.0, i: 0.0, i_clamp: 0.0}
      state_publish_rate:  50       # was 50. stabieler?
      stop_trajectory_duration: 0.0

